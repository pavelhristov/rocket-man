{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2019
    },
    "rules": {
        "eqeqeq": 2,
        "comma-dangle": 1,
        "no-console": 0,
        "no-debugger": 1,
        "no-extra-semi": 1,
        "no-extra-parens": 1,
        "no-irregular-whitespace": 1,
        "no-undef": 1,
        "no-unused-vars": 0,
        "semi": 1,
        "semi-spacing": 1,
        "valid-jsdoc": [
            2,
            {
                "requireReturn": false
            }
        ],
        "quotes": [
            "error",
            "single",
            {
                "allowTemplateLiterals": true
            }
        ]
    }
}